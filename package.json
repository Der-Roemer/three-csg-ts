{
  "name": "three-csg-ts",
  "version": "1.0.5",
  "description": "CSG library for use with THREE.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "lint": "eslint .",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "release": "standard-version",
    "prerelease": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jiro-Digital/three-csg-ts.git"
  },
  "keywords": [
    "three",
    "threejs",
    "three-js",
    "csg",
    "computational-solid-geometry"
  ],
  "author": "Jiro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jiro-Digital/three-csg-ts/issues"
  },
  "homepage": "https://github.com/Jiro-Digital/three-csg-ts#readme",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^12.19.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-jest": "^23.20.0",
    "husky": "^4.3.0",
    "jest": "^25.5.4",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "standard-version": "^9.0.0",
    "three": "^0.121.1",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "three": ">= 0.113.0 < 1"
  },
  "files": [
    "lib/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*/**": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
